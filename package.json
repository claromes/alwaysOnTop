{
  "name": "alwaysontop",
  "version": "0.1.0",
  "description": "A browser window always on top to help productivity",
  "homepage": "https://gitlab.com/claromes/alwaysontop",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "linux64": "electron-packager . alwaysontop --platform=linux --arch=x64",
    "deb64": "electron-installer-debian --src alwaysontop-linux-x64/ --dest dist/installers/ --arch amd64 --config config.json"
  },
  "author": "Claromes <claromes@protonmail.ch>",
  "devDependencies": {
    "electron": "^9.1.2",
    "electron-installer-debian": "^3.1.0",
    "electron-packager": "^15.1.0"
  }
}
